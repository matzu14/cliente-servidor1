<% include _header%>

    <h1><%= title %></h1>
    <p>Welcome to
      <%= title %>
    </p>
    
    <h2> Lista de zombies </h2>
    
    <Table class="table table-striped">
      <tr>
      <th>Nombre</th>
      <th>Email</th>
      <th>Tipo</th>
      </tr>
    

    <% 
      if(coleccion){
        coleccion.forEach(function(zombies){
      %>
      <tr>
        <td> <%= zombies.name %> </td>
        <td> <%= zombies.email %> </td>
        <td> <%= zombies.type %> </td>
      </tr>

      <%
        });}
      %>
    </Table>

<% include _footer %>
